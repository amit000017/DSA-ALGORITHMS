graph={
    '5':['3','7'],
    '3':['2','4'],
    '7':['8'],
    '2':[],
    '4':['8'],
    '8':[]
}

visted=[]
queue=[]

def bsf(visted,graph,node):
 visted.append(node)
 queue.append(node)

 while queue:
    m=queue.pop(0)
    print(m,end=" ")
    
    for neighbour in graph[m]:
        if neighbour not in visted:
            visted.append(neighbour)
            queue.append(neighbour)
            
print("The following breadth first search")
bsf(visted,graph,'5')
